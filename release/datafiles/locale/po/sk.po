msgid "Bring to Front"
msgstr "Preniesť dopredu"


msgid "Bring Forward"
msgstr "Prenášať vpred"


msgid "Send Backward"
msgstr "Posielať vzad"


msgid "Send to Back"
msgstr "Poslať dozadu"


msgctxt "Operator"
msgid "Set Caps Mode"
