msgstr "Đảo Nghịch UV"


msgid "Flip direction of UV coordinates inside faces"
msgstr "Đảo ngịch hướng yọa độ UV ở trong mặt"


msgctxt "Operator"
msgid "Rotate UVs"
msgstr "Quay UV"


msgid "Rotate UV coordinates inside faces"
msgstr "Quay tọa độ UV ở trong mặt"


msgctxt "Operator"
msgid "Vertex Connect"
msgstr "Kết Nối Đỉnh"


msgid "Connect selected vertices of faces, splitting the face"
msgstr "Kết nối đỉnh được chọn của mặt, cho chẻ mặt"


msgctxt "Operator"
msgid "Split Concave Faces"
msgstr "Chẻ Mặt Lõm"


msgid "Make all faces convex"
msgstr "Lăm hết mặt lồi"


msgctxt "Operator"
msgid "Split Non-Planar Faces"
msgstr "Chia Mặt Không Mặt Phẳng"


msgid "Split non-planar faces that exceed the angle threshold"
msgstr "Chia các mặt thì các đỉnh không nằm ở dưới giới hạn góc xem như mặt phẳng"


msgctxt "Operator"
msgid "Vertex Connect Path"
msgstr "Đường Dẫn Kết Nối Đỉnh"


msgid "Connect vertices by their selection order, creating edges, splitting faces"
msgstr "Nối 2 đỉnh của mặt bằng thứ tự được chọn, chẻ mặt "


msgctxt "Operator"
msgid "Add Vertex Color"
msgstr "Thêm Màu Đỉnh"


msgid "Add vertex color layer"
msgstr "Thêm lớp màu đỉnh"


msgctxt "Operator"
msgid "Remove Vertex Color"
msgstr "Bỏ Màu Đỉnh"


msgid "Remove vertex color layer"
msgstr "Bỏ lớp màu đỉnh"


msgctxt "Operator"
msgid "Smooth Vertex"
msgstr "Mịn Đỉnh"


msgid "Flatten angles of selected vertices"
msgstr "Băng Thẳng hóa góc của các đỉnh được chọn"


msgid "Smoothing factor"
msgstr "Trị Số Mịn Hóa"


msgid "Number of times to smooth the mesh"
msgstr "Số lần làm mịn lưới"


msgid "Smooth along the X axis"
msgstr "Mịn hướng trục X"


msgid "Smooth along the Y axis"
msgstr "Mịn hướng trục Y"


msgid "Smooth along the Z axis"
msgstr "Mịn hướng trục Z"


msgctxt "Operator"
msgid "Laplacian Smooth Vertex"
msgstr "Mịn Hóa Lapace Đỉnh"


msgid "Laplacian smooth of selected vertices"
msgstr "Mịn hoá kiểu Laplace các đỉnh được chọn"


msgid "Number of iterations to smooth the mesh"
msgstr "Số lượng lặp lặi cho mịn hóa lưới"


msgid "Lambda factor"
msgstr "Trị Số Lambda"


msgid "Smooth X Axis"
msgstr "Mịn Trục X"


msgid "Smooth Y Axis"
msgstr "Mịn Trị Số Y"


msgid "Smooth Z Axis"
msgstr "Mịn Trị Số Z"


msgctxt "Operator"
msgid "Wire Frame"
msgstr "Khung Dây"


msgid "Create a solid wire-frame from faces"
msgstr "Tạo khung dây rắn từ các mặt"


msgid "Remove original faces"
msgstr "Xóa các mặt gốc"


msgid "Crease weight"
msgstr "Quyền lượng nhăn"


msgctxt "Operator"
msgid "Add Mocap Fix"
msgstr "Thêm Chỉnh Chụp Chuyển Động"


msgid "Add a post-retarget fix - useful for fixing certain artifacts following the retarget"
msgstr "Thêm một chỉnh sửa mục tiêu sau -  tốt cho chỉnh sửa sai lầm sau chỉnh lại mục tiêu"


msgid "Type of Fix"
msgstr "Loại Chỉnh Sửa"


msgid "Type of fix"
msgstr "Loại chỉnh sửa"


msgid "Maintain Position"
msgstr "Giữ Vị Trí"


msgid "Bone is at a specific point"
msgstr "Xương ở tại điểm xác định"


msgid "Maintain Position at frame"
msgstr "Giữ Vị Trí Tại Bức Ảnh"


msgid "Bone does not move from location specified in target frame"
msgstr "Xương sẽ không di chuyển từ vị trí xác định trong bức ảnh mục đích"


msgid "Stay above"
msgstr "Giữ ở trên"


msgid "Bone does not cross specified mesh object eg floor"
msgstr "Xương sẽ không xuyên qua ật thể lưới xác định (vị dụ sàn nhà)"


msgid "Maintain distance"
msgstr "Giữ cách xa"


msgid "Target bones maintained specified distance"
msgstr "Xương mục tiêu giữ khoảng cách xác định"


msgctxt "Operator"
msgid "Stitch Animations"
msgstr "Mai Hoạt Hình"


msgid "Stitch two defined animations into a single one via alignment of NLA Tracks"
msgstr "Mai hai hoạt hình xác định thành một bằng sắp xếp của Lằn NLA"


msgctxt "Operator"
msgid "Guess Animation Stitch"
msgstr "Đoán Mai Hoạt Hình"


msgid "Guess the stitch frame and second offset for animation stitch"
msgstr "Đoán mai bức ảnh nào và dịch thứ hai cho mai hoạt hình"


msgctxt "Operator"
msgid "Bake Mocap Fixes"
msgstr "Nướng Chỉnh Chụp Chuyển Động"


msgid "Bake all post-retarget fixes to the Retarget Fixes NLA Track"
msgstr "Nướng hết chỉnh sửa mục tiêu sau đến Lằn NLA Chỉnh Sửa Mục Tiêu"


msgctxt "Operator"
msgid "Denoise Mocap"
msgstr "Giảm Huyên Náo Chụp Chuyển Động"


msgid "Removes spikes from all fcurves on the selected object"
msgstr "Xóa gai từ hết Cong-F của vật thể được chọn"


msgctxt "Operator"
msgid "Guess Hierarchy Mapping"
msgstr "Đoán Ánh Xạ Tầng Bậc"


msgid "Attempt to auto figure out hierarchy mapping"
msgstr "Thử tự động phát hiện ánh xạ tầng bậc"


msgctxt "Operator"
msgid "Set DOF Constraints"
msgstr "Đặt Ràng Buộc Hạn Chế"


msgid "Create limit constraints on the active armature from the selected armature's animation's range of motion"
msgstr "Tạo ràng buộc hạn chế cho cốt kích động từ phạm vi chuyển động hoạt hình của cốt được chọn"


msgctxt "Operator"
msgid "Load Mapping"
msgstr "Đọc Bản Đồ"


msgid "Load saved mapping from active armature"
msgstr "Đọc ánh xạ lưu từ cốt kích động"


msgctxt "Operator"
msgid "Loop Mocap"
msgstr "Lặp Vòng Chục Chuyển Động"


msgid "Trim active armature's animation to a single cycle, given a cyclic animation (such as a walk cycle)"
msgstr "Cắt bất hoạt hình cốt thành một chu trình, cho hoạt hình chu trình (như chu trình đi bộ)"


msgctxt "Operator"
msgid "Set Path"
msgstr "Đặt Đường Dẫn"


msgid "Set active object (stride object) to follow the selected curve"
msgstr "Đặt vật thể kích động (vật thể bước) cho theo cong được chọn"


msgctxt "Operator"
msgid "Remove Mocap Fix"
msgstr "Xóa Chỉnh Chục Chuyển Động"


msgid "Remove this post-retarget fix"
msgstr "Xóa chỉnh sửa mục tiêu sau này"


msgctxt "Operator"
msgid "Remove DOF Constraints"
msgstr "Xóa Ràng Buộc Hạn chế"


msgid "Remove previously created limit constraints on the active armature"
msgstr "Xóa các ràng buộc hạn chế được đặt rồi của cốt kích động"


msgctxt "Operator"
msgid "Retarget"
msgstr "Chỉnh Mục Tiêu"


msgid "Retarget animation from selected armature to active armature"
msgstr "Chỉnh mục tiêu hoạt hình từ cốt được chọn đến cốt kích động"


msgctxt "Operator"
msgid "Rotate Fix"
msgstr "Chỉnh Xoay"


msgid "Realign the active armature's axis system to match Blender (commonly needed after bvh import)"
msgstr "Sắp xếp lại hệ thống trục cốt cho giống hệ thống của Blender (ưa cần sau nhập bvh)"


msgctxt "Operator"
msgid "Convert Samples"
msgstr "Biến Đổi Mẫu Vật"


msgid "Convert active armature's sampled keyframed to beziers"
msgstr "Biến đổi mẫu vật bức ảnh mẫu của cốt kích động thành cong Bezier"


msgctxt "Operator"
msgid "Save Mapping"
msgstr "Giử Bản Đồ"


msgid "Save mapping to active armature (for future retargets)"
msgstr "Lưu ánh xạ vào cốt kích động (cho chỉnh mục tiêu tương lại)"


msgctxt "Operator"
msgid "Scale Fix"
msgstr "Chỉnh Phóng To"


msgid "Rescale selected armature to match the active animation, for convenience"
msgstr "Phóng to cốt được chọn cho giống hoạt hình kích động, cho tiện"


msgctxt "Operator"
msgid "Select Mapping Bone"
msgstr "Chọn Xương Ánh Xạ"


msgid "Select a bone for faster mapping"
msgstr "Chọn một xương để ánh xạ nhanh hơn"


msgctxt "Operator"
msgid "Unbake Mocap Fixes"
msgstr "Xóa Nướng Chỉnh Chụp Chuyển Động"


msgid "Unbake all post-retarget fixes - removes the baked data from the Retarget Fixes NLA Track"
msgstr "Xóa nướng cho hết hết chỉnh sửa mục tiêu sau - xóa dữ liệu nướng từ Lằn NLA Chỉnh Sửa Mục Tiêu"


msgctxt "Operator"
msgid "Update Mocap Fixes"
msgstr "Nâng Cấp Chỉnh Chụp Chuyển Động"


msgid "Update all post-retarget fixes (neccesary to take under consideration changes to armature object or pose)"
msgstr "Nâng cấp cho hết chỉnh sửa mục tiêu sau (cần quan tâm chỉnh đổi của vật thể cốt hay dạng dứng)"


msgid "Push action down onto the top of the NLA stack as a new strip"
msgstr "Đẩy hành động xuống trên xếp đống NLA làm một đoạn mới"


msgid "Channel Index"
msgstr "Chỉ Số Kênh"


msgid "Index of NLA action channel to perform pushdown operation on"
msgstr "Chỉ số kênh hành động NLA để thực hành thao tác đẩy xuống"


msgctxt "Operator"
msgid "Sync Action Length"
msgstr "Đồng Bộ Chiều Dài Kích Động"


msgid "Synchronize the length of the referenced Action with the length used in the strip"
msgstr "Đồng bộ chiều dài của Hành Động tham chiếu với chiều dài xài trong đoạn"


msgid "Active Strip Only"
msgstr "Chỉ Đoạn Hành Động"


msgid "Only sync the active length for the active strip"
msgstr "Chỉ đồng bộ ciều dài kích động cho đaọn kích động"


msgctxt "Operator"
msgid "Add Action Strip"
msgstr "Thêm Đoạn Hành Động"


msgid "Add an Action-Clip strip (i.e. an NLA Strip referencing an Action) to the active track"
msgstr "Thêm một đoạn Khúc Hành Động (ví dụ Đoạn NLA tham chiếu một Hành Động) vào lằn kích động"


msgctxt "Operator"
msgid "Apply Scale"
msgstr "Áp Dụng Phóng To"


msgid "Apply scaling of selected strips to their referenced Actions"
msgstr "Áp dụng phóng to của các đoạn được chọn đến Hành Động tham chiếu của chúng nó"


msgctxt "Operator"
msgid "Bake Action"
msgstr "Nướng Hành Động"


msgid "Bake object/pose loc/scale/rotation animation to a new action"
msgstr "Nướng hoạt hình vật thể/dạng đứng vị trí/phóng to/xoay đến một hành động mới"


msgid "Only key selected object/bones"
msgstr "Chỉ làm bức ảnh mẫu cho vật thể/xương được chọn"


msgid "Visual Keying"
msgstr "Hiển Thị Bức Ảnh Mẫu"


msgid "Keyframe from the final transformations (with constraints applied)"
msgstr "Bức ảnh mẩu từ biến hóa cuối (có áp dụng ràng buộc)"


msgid "Clear Constraints"
msgstr "Xóa Ràng Buộc"


msgid "Remove all constraints from keyed object/bones, and do 'visual' keying"
msgstr "Xóa hết ràng buộc từ vật thể/xương có bức ảnh mẫu, và làm bức ảnh mẫu 'thị giác'"


msgid "Clear Parents"
msgstr "Xóa Phụ Huynh"


msgid "Bake animation onto the object then clear parents (objects only)"
msgstr "Nướng hoạt hình trong vật thể khi xóa phụ huynh (chỉ cho vật thể)"


msgid "Overwrite Current Action"
msgstr "Ghi Lên Hành Động Hiện Tại"


msgid "Bake animation into current action, instead of creating a new one (useful for baking only part of bones in an armature)"
msgstr "Nướng hành động vào hành động hiện tại, thay tạo nên một cái mới (xài cho chỉ nướng một phần xương trong một cốt)"


msgid "Which data's transformations to bake"
msgstr "Nướng biến hóa của dữ liệu nào"


msgid "Bake bones transformations"
msgstr "Nướng biến hóa xương"


msgid "Bake object transformations"
msgstr "Nướng biến hóa vật thể"


msgctxt "Operator"
msgid "Mouse Click on NLA Channels"
msgstr "Bấm Chuột Trên Kênh NLA"


msgid "Handle clicks to select NLA channels"
msgstr "Xài chuột đề chọn kênh NLA"


msgctxt "Operator"
msgid "Clear Scale"
msgstr "Xóa Phóng to"


msgid "Reset scaling of selected strips"
msgstr "Đặt lại phóng to của đoạn được chọn"


msgctxt "Operator"
msgid "Mouse Select"
msgstr "Chuột Lựa Chọn"


msgid "Handle clicks to select NLA Strips"
msgstr "Xài chuột để chọn Đoạn NLA"


msgctxt "Operator"
msgid "Delete Strips"
msgstr "Xóa Đoạn"


msgid "Delete selected strips"
msgstr "Xóa đoạn được chọn"


msgctxt "Operator"
msgid "Duplicate Strips"
msgstr "Sao Chép Đoạn"


msgid "Duplicate selected NLA-Strips, adding the new strips in new tracks above the originals"
msgstr "Sao chép Đaọn NLA được chọn, thêm các đoạn mới trong lằn mới ở trên đoạn ban đầu"


msgid "When duplicating strips, assign new copies of the actions they use"
msgstr "Khi sao chép đoạn, chỉ định sao chép mới của hành động chúng nó xài"


msgctxt "Operator"
msgid "Add F-Modifier"
msgstr "Thêm Cụ Sửa Đổi F"


msgid "Add a F-Modifier of the specified type to the selected NLA-Strips"
msgstr "Thêm một Cụ Sửa Đổi F của loại xác định vào Đoạn NLa được chọn"


msgid "Only add a F-Modifier of the specified type to the active strip"
msgstr "Chỉ thêm một Cụ Sửa Đổi có loại xác định vào đoạn kích động"


msgid "Copy the F-Modifier(s) of the active NLA-Strip"
msgstr "Chép các Cụ Sửa Đổi F của Đoạn NLA kích động"


msgid "Add copied F-Modifiers to the selected NLA-Strips"
msgstr "Thêm Cụ Sửa Đổi F chép vào Đoạn NLA được chọn"


msgctxt "Operator"
msgid "Make Single User"
msgstr "Làm Chỉ Xài Một Lần"


msgid "Ensure that each action is only used once in the set of strips selected"
msgstr "Làm chắc mỗi hành động chỉ được xài một lần trong tập đoạn được chọn"


msgctxt "Operator"
msgid "Add Meta-Strips"
msgstr "Thêm Siêu Đoạn"


msgid "Add new meta-strips incorporating the selected strips"
msgstr "Thêm siêu đoạn mới gồm các đoạn được chọn"


msgctxt "Operator"
msgid "Remove Meta-Strips"
msgstr "Xóa Siêu Đoạn"


msgid "Separate out the strips held by the selected meta-strips"
msgstr "Chia ra các đoạn được giữ trong các siêu đoạn chọn chọn"


msgctxt "Operator"
msgid "Move Strips Down"
msgstr "Di Chuyển Đoạn Xuống"


msgid "Move selected strips down a track if there's room"
msgstr "Di chuyển đoạn xuống một lằn nếu có chổ đi"


msgctxt "Operator"
msgid "Move Strips Up"
msgstr "Di Chuyển Đoạn Lên"


msgid "Move selected strips up a track if there's room"
msgstr "Di chuyển đoạn được chọn lên một lằn nếu có chổ đi"


msgctxt "Operator"
msgid "Toggle Muting"
msgstr "Bật/Tắt Cắt Tiếng"


msgid "Mute or un-mute selected strips"
msgstr "Cắt hay không cắt tiếng đoạn được chọn"


msgid "Select or deselect all NLA-Strips"
msgstr "Chọn hay thả hết đoạn NLA"


msgid "Use box selection to grab NLA-Strips"
msgstr "Xài hộp lựa chọn để chọn Đoạn NLA"


msgid "Select strips to the left or the right of the current frame"
msgstr "Chọn đoạn phía trái hay phía phải của bức ảnh hiện tại"


msgctxt "Operator"
msgid "Include Selected Objects"
msgstr "Gồm Vật Thể Được Chọn"


msgid "Make selected objects appear in NLA Editor by adding Animation Data"
msgstr "Làm vật thể đơực chọn xuất hiện trong Soan Thảo NLA bằng thêm Dữ Liệu Hoạt Hình"


msgctxt "Operator"
msgid "Snap Strips"
msgstr "Hút Dính Đoạn"


msgid "Move start of strips to specified time"
msgstr "Di chuyển đầu của đoạn đến thời gian xác định"


msgctxt "Operator"
msgid "Add Sound Clip"
msgstr "Thêm Đoạn Âm Thanh"


msgid "Add a strip for controlling when speaker plays its sound clip"
msgstr "Thêm một đoạn cho điều khiểu khi loa sẽ hát âm thanh củ nó"


msgctxt "Operator"
msgid "Split Strips"
msgstr "Chẻ Đoạn"


msgid "Split selected strips at their midpoints"
msgstr "Chẻ đoạn được chọn tạo trung điểm"


msgctxt "Operator"
msgid "Swap Strips"
msgstr "Đổi Đoạn"


msgid "Swap order of selected strips within tracks"
msgstr "Đổi thứ tự của đoạn được chọn trong lằn"


msgctxt "Operator"
msgid "Add Tracks"
msgstr "Thêm Lằn"


msgid "Add NLA-Tracks above/after the selected tracks"
msgstr "Thêm Lằn NLA trên/sau các làn được chọn"


msgid "Above Selected"
msgstr "Trên Được Chọn"


msgid "Add a new NLA Track above every existing selected one"
msgstr "Thêm một lằn NLA trên mối lằn tồn tại được chọn"


msgctxt "Operator"
msgid "Delete Tracks"
msgstr "Xóa Lắn"


msgid "Delete selected NLA-Tracks and the strips they contain"
msgstr "Xoá  các lằn NLA được chọn và các đoạn nội dung của chúng"


msgctxt "Operator"
msgid "Add Transition"
msgstr "Thêm Tiến Triển"


msgid "Add a transition strip between two adjacent selected strips"
msgstr "Thêm một đoạn tiến triển giữa hai đaọn kề được chọn"


msgctxt "Operator"
msgid "Enter Tweak Mode"
msgstr "Vào Chế Độ Chỉnh"


msgid "Enter tweaking mode for the action referenced by the active strip to edit its keyframes"
msgstr "Vào chế độ chỉnh của hành động được đoạn kích động tham chiếu để soạn thảo bức ảnh mẫu của nó"


msgid "Isolate Action"
msgstr "Độc Lập Hành Động"


msgid "Enable 'solo' on the NLA Track containing the active strip, to edit it without seeing the effects of the NLA stack"
msgstr "Bật 'độc lập' trên Lằn NLA có đoạn kích động, đề soạn thảo nó mà không thấy hiệu ứng của xếp đống NLA"


msgctxt "Operator"
msgid "Exit Tweak Mode"
msgstr "Thoát Chế ĐỘ Chỉnh"


msgid "Exit tweaking mode for the action referenced by the active strip"
msgstr "Thóat chế độ chỉnh của hành động được đoạn kích động tham chiếu"


msgid "Disable 'solo' on any of the NLA Tracks after exiting tweak mode to get things back to normal"
msgstr "Tắt 'độc lập' cho bắt cư Lằn NLA nào sau thoát chế độ chỉnh để dẫn đồ quya trở lại bình thường"


msgid "Reset viewable area to show full strips range"
msgstr "Đặt lại vùng có thể nhìn cho chiếu toàn phạm vi của các đoạn"


msgid "Reset viewable area to show selected strips range"
msgstr "Đặt lại vùng được thấy cho chiếu phạm vị của các đoạn được chọn"


msgctxt "Operator"
msgid "Add and Link Node"
msgstr "Thêm Và Nối Giao Điểm"


msgid "Add a node to the active tree and link to an existing socket"
msgstr "Thêm một giao điểm vào cây kích động và nối nó với ổ cắm có sẵn"


msgid "Node Type"
msgstr "Loại Giao Điểm"


msgid "Node type"
msgstr "Loại giao điểm"


msgid "Use Transform"
msgstr "Xài Biến Hóa"


msgid "Start transform operator after inserting the node"
msgstr "Bắt đầu tham tác biến hóa sau chèn giao điểm"


msgid "Settings to be applied on the newly created node"
msgstr "Đặt trưng áp dụng cho giao đểm mới tạo"


msgid "Link Socket Index"
msgstr "Nối Chỉ Số Ổ Cắm"


msgid "Index of the socket to link"
msgstr "Chỉ số của ổ cắm để nối"


msgctxt "Operator"
msgid "Add File Node"
msgstr "Thêm Giao Điểm Tệp"


msgid "Add a file node to the current node editor"
msgstr "Thêm một giro điê ̉m tệp trong soạn thảo giao điểm hiện tại"


msgid "Datablock name to assign"
msgstr "Tên cục dữ liệu để chỉ định"


msgctxt "Operator"
msgid "Add Mask Node"
msgstr "Thêm Giao Điểm Mặt Nạ"


