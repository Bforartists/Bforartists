msgctxt "Operator"
msgid "Enable Channel Setting"
msgstr "Bật Đặc Trưng Hoạt Hình"


msgid "Enable specified setting on all selected animation channels"
msgstr "Bật đặc trưng xác định của hết kênh hoạt hình được chọn"


msgctxt "Operator"
msgid "Toggle Channel Setting"
msgstr "Bật/Tắt Đặc Trưng Kênh"


msgid "Toggle specified setting on all selected animation channels"
msgstr "Bật/tắt đặc trưng được xác định của hết kênh hoạt hình được chọn"


msgctxt "Operator"
msgid "Ungroup Channels"
