#include <stdosl.h>

shader noise(
	float Time = 1.0,
	point Point = P,
	output float Cell = 0.0,
	output color Perlin = 0.8,
	output color UPerlin = 0.8)
{   
	/* Cell Noise */
	Cell = noise("cell", Point);
	
    /* Perlin 4D Noise*/
    Perlin = noise("perlin", Point, Time);
   
    /* UPerlin 4D Noise*/
    UPerlin = noise("uperlin", Point, Time);
}