#!/usr/bin/python

Import('env')

sources = env.Glob('Recast/Source/*.cpp') + env.Glob('Detour/Source/*.cpp')

incs = 'Recast/Include Detour/Include'

env.BlenderLib ( 'extern_recastnavigation', sources, Split(incs), [],
                 libtype=['extern','player'],
                 priority=[10,185])
